
.text{
  display: flex;
  font-size: 24rpx;
  margin-top: 26rpx;
  justify-content: space-between;
  width: 710rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
}
.text1{
  color: #fe0000;
}
.shop{
  margin-bottom: 20rpx;
}
.shopping{
  position: relative;
  display: flex;
}
.shop_img{
  height: 150rpx;
  width: 150rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
}
.shop_text{
  display: flex;
}
.text3{
  font-size: 28rpx;
  color: #333;
  height: 150rpx;
  line-height: 50rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.tetx4{
  display: flex;
