/* pages/invoice/list.wxss */
.myInvoice{
  width: 96%;
  float: left;
  margin-left: 2%;
}
.myInvoiceLi{
  width: calc(100% - 40rpx);
  padding: 20rpx;
  float: left;
  background: #fff;
  margin-top: 20rpx;
  border-radius: 10px;
}
.myInvoiceLiT1{
  width: 100%;
  float: left;
  font-size: 28rpx;
  color: #333;
}
.myInvoiceLiT2{
  width: 50%;
  float: left;
  font-size: 24rpx;
  color: #c0c0c0;
  margin-top: 4rpx;
}
.myInvoiceLiT3{
  width: 100%;
  float: left;
  margin-top: 20rpx;
  padding-top: 20rpx;
  border-top: 1px solid #f7f7f7;
}
.myInvoiceLiT3_t1{
  float: left;
  font-size: 24rpx;
  color: #333;
}
.myInvoiceLiT3_t1 text{
  font-size: 30rpx;
  color: #FE0000;
}
.myInvoiceLiT3_t2{
  float: right;
  font-size: 22rpx;
  color: #333;
}
.listAdd{
  position: fixed;
  right: 20rpx;
  bottom: 100rpx;
  z-index: 9;
  width: 60rpx;
}